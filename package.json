{
  "name": "dudu-editor",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run lint && webpack --progress --mode=production --config ./build/webpack.config.js --env production && echo \"--------Build Success!--------\"",
    "start": "webpack-dev-server --mode=development --config ./build/webpack.config.js --env development --host 0.0.0.0",
    "clean": "node ./node_modules/rimraf/bin.js ./dist/*",
    "lint": "npm run elint && npm run slint",
    "lintFix": "npm run elintFix && npm run slintFix ",
    "elint": "npx eslint \"./src/components/**/*.vue\"",
    "elintFix": "npx eslint \"./src/components/**/*.vue\" --fix",
    "slint": "npx stylelint \"./src/components/**/*.scss\"",
    "slintFix": "npx stylelint \"./src/components/**/*.scss\" --fix"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/youngchou1997/dudu-editor.git"
  },
  "keywords": [
    "component"
  ],
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-component": "^1.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cpx": "^1.5.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^4.7.1",
    "file-loader": "^6.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "sass": "^1.26.5",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
